<UserControl x:Class="DataVisualization.DataPlotter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
             mc:Ignorable="d" d:DesignWidth="482.707" Height="274.906" d:DataContext="{d:DesignData /SampleData/VisualizationViewModelSampleData.xaml}">
    <Grid>
        <d3:ChartPlotter Name="ChartControl">
			<d3:Header Name="Header" Content="sdfsdaf"/>
            <d3:VerticalAxisTitle Name="VerticalAxisLabel" Content="{Binding VertivalAxisLabel}"/>
            <d3:HorizontalAxisTitle Name="HorizontalAxisLabel" Content="{Binding HorizontalAxisLabel}" />
			
        </d3:ChartPlotter>
        <!--<d3:Legend Name="LegendVisibility" Visibility="{Binding Path=LegendVisibility, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <d3:VerticalAxisTitle Name="VerticalAxisLabel" Content="{Binding Path=VerticalAxisLabel, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <d3:HorizontalAxisTitle Name="HorizontalAxisLabel" Content="{Binding Path=HorizontalAxisLabel, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />-->
    </Grid>
</UserControl>
